{
  "event": {
    "provider": "github-action-build",
    "reason": "Job triggered",
    "url": "JOB_URL"
  },
  "actions": [
    {
      "action": "package-build",
      "id": "build",
      "provision": [],
      "service": {
        "project": "<%= projectName %>",
        "name": "<%= serviceName %>",
        "environment": "tools"
      },
      "package": {
        "architecture": "<%= packageArchitecture %>",
        "version": "",
        "buildVersion": "",
        "buildNumber": 0,
        "name": "<%= serviceName %>",
        "type": "<%= packageType %>",
        "license": "<%= license %>"
      }
    }
  ],
  "user": {
    "name": "github@internal"
  }
}