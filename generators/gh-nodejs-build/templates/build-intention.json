{
  "event": {
    "provider": "github-action-build",
    "reason": "Job triggered",
    "url": "JOB_URL"
  },
  "actions": [
    {
      "action": "package-build",
      "id": "build",
      "provision": [],
      "service": {
        "project": "<%= projectName %>",
        "name": "<%= serviceName %>",
        "environment": "tools"
      },
      "package": {
        "version": "",
